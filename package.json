{
  "name": "n8n-nodes-supabase-memory",
  "version": "0.1.0",
  "description": "n8n community node for AI tool agent memory with Supabase integration",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "supabase",
    "ai",
    "memory",
    "agent"
  ],
  "license": "MIT",
  "homepage": "https://github.com/dwillitzer/n8n-nodes-supabase-memory",
  "author": {
    "name": "dwillitzer",
    "email": "daniel@willitzer.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwillitzer/n8n-nodes-supabase-memory.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts",
    "lintfix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js src"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SupabaseApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SupabaseMemory/SupabaseMemory.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.19.101",
    "@types/request-promise-native": "^1.0.18",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-plugin-n8n-nodes-base": "^1.13.0",
    "gulp": "^4.0.2",
    "n8n-core": "^0.157.0",
    "n8n-workflow": "^0.147.0",
    "prettier": "^2.8.8",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.21.0",
    "uuid": "^11.1.0"
  }
}
